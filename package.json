{
	"name": "natours",
	"version": "1.0.0",
	"description": "Learning node, express and mongoDB",
	"main": "app.js",
	"scripts": {
		"start:dev": "nodemon server.js",
		"start:prod": "set NODE_ENV=production&& nodemon server.js",
		"debug": "ndb server.js",
		"watch:js": "parcel watch ./public/js/index.js --out-dir ./public/js --out-file bundle.js --public-url /js",
		"build:js": "parcel build ./public/js/index.js --out-dir ./public/js --out-file bundle.js --public-url /js"
	},
	"author": "Jonas Schmedtmann",
	"license": "ISC",
	"dependencies": {
		"axios": "latest",
		"bcryptjs": "latest",
		"compression": "latest",
		"cookie-parser": "latest",
		"core-js": "latest",
		"dotenv": "latest",
		"express": "latest",
		"express-mongo-sanitize": "latest",
		"express-rate-limit": "latest",
		"helmet": "latest",
		"hpp": "latest",
		"html-to-text": "latest",
		"is-buffer": "^2.0.5",
		"jsonwebtoken": "latest",
		"mongoose": "latest",
		"morgan": "latest",
		"multer": "latest",
		"nodemailer": "latest",
		"parcel": "latest",
		"pug": "latest",
		"regenerator-runtime": "^0.14.1",
		"sharp": "latest",
		"slugify": "latest",
		"stripe": "latest",
		"validator": "latest",
		"xss-clean": "latest"
	},
	"engines": {
		"node": ">=10.0.0"
	}
